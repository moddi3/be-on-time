{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "dist/**"],
			"env": [
				"NODE_ENV",
				"DATABASE_URL",
				"NEXTAUTH_SECRET",
				"NEXTAUTH_URL",
				"GOOGLE_CLIENT_ID",
				"GOOGLE_CLIENT_SECRET",
				"VERCEL_URL",
				"PORT"
			]
		},
		"dev": {
			"cache": false
		},
		"lint": {
			"outputs": []
		}
	}
}
